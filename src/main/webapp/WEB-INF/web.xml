<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
<display-name>QuizHub</display-name>
<servlet>
  <servlet-name>LogInServlet</servlet-name>
  <servlet-class>Servlets.LogInServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>RegisterServlet</servlet-name>
  <servlet-class>Servlets.RegisterServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>MultiPageServlet</servlet-name>
  <servlet-class>Servlets.MultiPageServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>OnePageServlet</servlet-name>
  <servlet-class>Servlets.OnePageServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>QuizCreateServlet</servlet-name>
  <servlet-class>Servlets.QuizCreateServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>QuestionCreateServlet</servlet-name>
  <servlet-class>Servlets.QuestionCreateServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>HomePageServlet</servlet-name>
  <servlet-class>Servlets.HomePageServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>QuizSummaryServlet</servlet-name>
  <servlet-class>Servlets.QuizSummaryServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>QuizResultServlet</servlet-name>
  <servlet-class>Servlets.QuizResultServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>ProfileServlet</servlet-name>
  <servlet-class>Servlets.ProfileServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>SearchServlet</servlet-name>
  <servlet-class>Servlets.SearchServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>AnnouncementCreateServlet</servlet-name>
  <servlet-class>Servlets.AnnouncementCreateServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>FriendRequestsServlet</servlet-name>
  <servlet-class>Servlets.FriendRequestsServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>ChatServlet</servlet-name>
  <servlet-class>Servlets.ChatServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>ChallengeFriendServlet</servlet-name>
  <servlet-class>Servlets.ChallengeFriendServlet</servlet-class>
</servlet>
<servlet>
  <servlet-name>ChatSearchServlet</servlet-name>
  <servlet-class>Servlets.ChatSearchServlet</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>LogInServlet</servlet-name>
  <url-pattern>/LogIn</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>RegisterServlet</servlet-name>
  <url-pattern>/Register</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>MultiPageServlet</servlet-name>
  <url-pattern>/MultiPage</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>OnePageServlet</servlet-name>
  <url-pattern>/OnePage</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>QuizCreateServlet</servlet-name>
  <url-pattern>/QuizCreate</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>QuestionCreateServlet</servlet-name>
  <url-pattern>/QuestionCreate</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>HomePageServlet</servlet-name>
  <url-pattern>/HomePage</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>QuizSummaryServlet</servlet-name>
  <url-pattern>/QuizSummary</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>QuizResultServlet</servlet-name>
  <url-pattern>/QuizResult</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>ProfileServlet</servlet-name>
  <url-pattern>/Profile</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>SearchServlet</servlet-name>
  <url-pattern>/Search</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>AnnouncementCreateServlet</servlet-name>
  <url-pattern>/AnnouncementCreate</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>FriendRequestsServlet</servlet-name>
  <url-pattern>/FriendRequests</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>ChatServlet</servlet-name>
  <url-pattern>/Chat</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>ChallengeFriendServlet</servlet-name>
  <url-pattern>/ChallengeFriend</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>ChatSearchServlet</servlet-name>
  <url-pattern>/ChatSearch</url-pattern>
</servlet-mapping>
</web-app>
